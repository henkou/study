#include<iostream>
using namespace std;
int main() {
	int x, y, n;
	x = 0;
	y = 0;
	cin >> n;
	int arr[19][19] = { };
	for (int i = 1; i <= n; i++) {
		cin >> x >> y;
		arr[x][y] = 1;
	}
	for (int i = 1; i <=18; i++) {
		for (int j = 1; j <= 18; j++) {
			cout << arr[i][j] << " ";
		}
		cout << endl;
	}
}
