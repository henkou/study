import java.io.*;
class Main {
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String input = br.readLine();
		int num = Integer.parseInt(input);
		for(int i = 0; i < num; i++){
    // 여기가 Point... 
			for(int j = num - i; j > 0; j--){
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
